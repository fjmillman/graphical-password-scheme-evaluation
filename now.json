{
  "version": 2,
  "name": "graphical-password-scheme-evaluation",
  "builds": [
    {
      "src": "api/routes/index.js",
      "use": "@now/node"
    },
    {
      "src": "www/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/routes"
    },
    {
      "src": "^/(.*)$",
      "dest": "/www/$1"
    }
  ],
  "alias": ["gpa-evaluation"]
}
